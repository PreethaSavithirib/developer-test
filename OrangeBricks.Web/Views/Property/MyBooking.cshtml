@model IEnumerable<OrangeBricks.Web.Controllers.Property.ViewModels.BookingListViewModel>
@{
    ViewBag.Title = "My Booking";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>My Properties</h2>
<p>Here is a list of your Booking:</p>
@foreach (var booking in Model)
{
    if (booking.BookingStatus == 0)
    {
        <div class="row">
            <div class="col-md-10">Booking view for date @booking.ViewDate is in pending status. </div>
        </div>
    }
}
